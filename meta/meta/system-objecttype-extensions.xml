<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">
    <type-extension type-id="ProductInventoryRecord">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="brainCommerceLastExportedPriceAndInventory">
                <display-name xml:lang="x-default">Brain Commerce</display-name>
                <type>text</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
        </custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="brainCommerce">
                <display-name xml:lang="x-default">Brain Commerce</display-name>
                <attribute attribute-id="brainCommerceLastExportedPriceAndInventory"/>
            </attribute-group>
        </group-definitions>
    </type-extension>
    <type-extension type-id="SitePreferences">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="isBrainCommerceBackendEnabled">
                <display-name xml:lang="x-default">Is Ingestion Enabled</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>false</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="brainCommerceIngestorAPIKey">
                <display-name xml:lang="x-default">Ingestor API Key</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="brainCommerceIngestorAPIUrl">
                <display-name xml:lang="x-default">Ingestor API URL</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="brainCommerceFAQCustomObjectID">
                <display-name xml:lang="x-default">Brain Commerce FAQ Custom Object ID</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>brainCommerceFAQ</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="brainCommerceDeltaExportBufferTime">
                <display-name xml:lang="x-default">Buffer Time For Product Delta Export (In Seconds)</display-name>
                <description xml:lang="x-default">Buffer time to compare product last modified time and brain commerce last export time.</description>
                <type>int</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>60</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="brainCommerceDeltaExportBufferTimeFAQ">
                <display-name xml:lang="x-default">Buffer Time For FAQ Delta Export (In Seconds)</display-name>
                <description xml:lang="x-default">Buffer time to compare FAQ last modified time and brain commerce last export time.</description>
                <type>int</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>60</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="brainCommerceProductAttributeMapping">
                <display-name xml:lang="x-default">Product Attribute Mapping</display-name>
                <type>text</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="isBrainCommerceFrontendEnabled">
                <display-name xml:lang="x-default">Is Conversation UI Enabled</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>false</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="brainCommerceFrontendAPIKey">
                <display-name xml:lang="x-default">Frontend API Key</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="brainCommerceFrontendAPIUrl">
                <display-name xml:lang="x-default">Frontend API URL</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="brainCommerceSDKURL">
                <display-name xml:lang="x-default">SDK URL</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>https://storage.googleapis.com/brain-commerce-static-eu-dev/rezolve-chatbot.js</default-value>
            </attribute-definition>

        </custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="BrainCommerceConfig">
                <display-name xml:lang="x-default">Brain Commerce Configurations</display-name>
                <attribute attribute-id="isBrainCommerceBackendEnabled"/>
                <attribute attribute-id="brainCommerceIngestorAPIKey"/>
                <attribute attribute-id="brainCommerceIngestorAPIUrl"/>
                <attribute attribute-id="brainCommerceFAQCustomObjectID"/>
                <attribute attribute-id="brainCommerceDeltaExportBufferTime"/>
                <attribute attribute-id="brainCommerceDeltaExportBufferTimeFAQ"/>
                <attribute attribute-id="brainCommerceProductAttributeMapping"/>
                <attribute attribute-id="isBrainCommerceFrontendEnabled"/>
                <attribute attribute-id="brainCommerceFrontendAPIKey"/>
                <attribute attribute-id="brainCommerceFrontendAPIUrl"/>
                <attribute attribute-id="brainCommerceSDKURL"/>
            </attribute-group>
        </group-definitions>
    </type-extension>

</metadata>
